<!DOCTYPE html>
	<html>
	<head>
		<title><%= full_title(yield(:title)) %></title>
	    <%= stylesheet_link_tag    "application", media: "all" %>
	    <%= javascript_include_tag "application" %>
	    <%= csrf_meta_tags %>
	</head>
	<body>

		<div class="outer">
		<div class="wrapper">

			<div class="logo">
			<%= link_to "sample app", '#', id: "logo" %>
			</div> <!-- end logo -->

			<div class="top-nav">
				<%= render 'layouts/top-nav' %>
			</div> <!-- end top-nav -->

			<div class="left-nav">
				<%= render 'layouts/left-nav' %>
			</div> <!-- end left-nav -->

			<div class="right-nav">
				<%= render 'layouts/right-nav' %>
			</div> <!-- end right-nav -->

			<div class="content" class="annoucement">

			</div> <!-- end content, annoucement -->

			<div class="footer">
				<%= render 'layouts/footer' %>
			</div> <!-- end footer -->

	<%= yield %>
		</div> <!-- end wrapper -->
		</div> <!-- end outer -->


	</body>
	</html>
